<script>
$(function(){



        $(".js-display-edit-icon").each( function(){             
             helpers.editIcon(this);
         });

        $("body").delegate('.js-edit-icon', 'click', function(){window.location = '/pharamchicals/edit/id/'+$(this).attr('phrama:id');});
         
});
        









</script>
         
